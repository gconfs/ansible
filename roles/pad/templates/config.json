{
    "production": {
        "domain": "pad.gconfs.fr",
        "host": "127.0.0.1",
        "protocolUseSSL": true,
        "hsts": {
            "enable": true,
            "maxAgeSeconds": "31536000",
            "includeSubdomains": true,
            "preload": true
        },
        "csp": {
            "enable": true,
            "directives": {
            },
            "upgradeInsecureRequests": "auto",
            "addDefaults": true,
            "addDisqus": false,
            "addGoogleAnalytics": false
        },
        "db": {
            "username": "codimd",
            "password": "{{ mysql_pass }}",
            "database": "codimd",
            "host": "localhost",
            "port": "3306",
            "dialect": "mysql"
        },
        "epita": {
    	    "clientID": "{{ epita_key }}",
    	    "clientSecret": "{{ epita_secret }}"
  	},
        "sessionSecret": "randomgconfssessionsecret",
        "allowEmailRegister": false,
  	"openID": false,
  	"allowAnonymous": false
    }
}

